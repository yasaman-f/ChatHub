        <main class="chat-window">
            <h2>Chat Window</h2>
            <div class="messages">
                <p>Hello!</p>
                <p>Hi there!</p>
            </div>
            <input type="text" placeholder="Type a message..." id="message-input">
            <button id="send-button">Send</button>
            <button id="File-button">File</button>
            <button id="GPS-button">GPS</button>
        </main>
    </div>
    <div id="overlay"></div>
    <div id="popupForm" class="popup">
        <form>
            <label for="name">UserName:</label>
            <input type="text" id="name" name="name" required>
            <label for="email">Email:</label>
            <input type="email" id="email" name="email" required>
            <button type="submit">Save</button>
        </form>
    </div>
    <ul id="friendsList" style="display: none;">
        <li id="add-room">Add room</li>
    </ul>
    <ul class="NamespaceList" id="NamespaceList" style="display: none;">
        
    </ul>
